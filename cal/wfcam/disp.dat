# Send raw frame to Gaia window 
# Need the num entry to display the raw data... <sigh>

num type=image tool=gaia region=0 window=0 autoscale=1 zautoscale=1
raw type=image tool=gaia region=0 window=0 autoscale=1 zautoscale=1

# Mosaic frames too

st type=image tool=gaia region=0 window=2 autoscale=1 zautoscale=1
sf type=image tool=gaia region=0 window=0 autoscale=1 zautoscale=1

