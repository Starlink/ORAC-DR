#_SCUBA2_HELLO_

_ALIGN_ARRAYS_

_MAKE_MOSAIC_SUBARRAY_

_MAKE_MOSAIC_FRAME_ METHOD=MEAN

#_EXTINCTION_CORRECT_FRAME_ EXTCOR_MODE=FULL

#_DISPLAY_FRAME_

#_CALIBRATE_DATA_

_MAKE_MOSAIC_GROUP_ COADD_MODE=RUNNING

_FIND_VARIANCE_

#_FIND_TOTAL_FLUX_ USEPSF=0

_DISPLAY_GROUP_

